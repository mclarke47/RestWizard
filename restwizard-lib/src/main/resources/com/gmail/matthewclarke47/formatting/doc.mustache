<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>
    <script src="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/js/bootstrap.min.js"></script>
    <script>
        $(document).ready(function () {

            function emptyTables() {
                return $("table").filter(function (index) {
                    return $(this).find("tbody").children().length == 0
                })
            }

            var emptyPres = $('pre:empty');
            emptyPres.prev().hide();
            emptyPres.hide();

            emptyTables().hide();
            emptyTables().prev().hide();

        });</script>
</head>
<body>
<div class="container">
    <div class="page-header"><h1>Rest Wizard test app</h1></div>
    {{#resources}}
        <div class="row">
            <div class="row">
                <h2>{{path}}</h2>
            </div>
            <div class="row">
                {{#getMethods}}
                    <div class="row">
                        <div class="panel panel-success">
                            <div class="panel-heading"><strong>GET</strong>   {{pathSuffix}}</div>
                            <div class="panel-body">
                                <div class="row ">
                                    {{>queryParams}}
                                    {{>pathParams}}
                                    {{>responseExample}}
                                </div>
                            </div>
                        </div>
                    </div>
                {{/getMethods}}
                {{#postMethods}}
                    <div class="row">
                        <div class="panel panel-primary">
                            <div class="panel-heading"><strong>POST</strong>   {{pathSuffix}}</div>
                            <div class="panel-body">
                                <div class="row ">
                                    {{>queryParams}}
                                    {{>pathParams}}
                                    {{>responseExample}}

                                </div>

                            </div>
                        </div>
                    </div>
                {{/postMethods}}
                {{#putMethods}}
                    <div class="row">
                        <div class="panel panel-info">
                            <div class="panel-heading"><strong>PUT</strong>   {{pathSuffix}}</div>
                            <div class="panel-body">
                                <div class="row ">
                                    {{>queryParams}}
                                    {{>pathParams}}
                                    {{>responseExample}}
                                </div>
                            </div>
                        </div>
                    </div>
                {{/putMethods}}
            </div>
        </div>
    {{/resources}}
</div>
</body>
</html>