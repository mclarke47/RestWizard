<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>
    <script src="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/js/bootstrap.min.js"></script>
    <script>
        $(document).ready(function () {

            function emptyTables(){
                return $("table").filter(function (index) {
                    return $(this).find("tbody").children().length == 0
                })
            }

            emptyTables().hide();
            emptyTables().prev().hide();

        });</script>
</head>
<body>
<div class="container">
    <div class="page-header"><h1>Rest Wizard test app</h1></div>

    {{#resources}}
        <h2>{{path}}</h2>

        <div class="row">
            {{#getMethods}}
                <div class="row">
                    <div class="panel panel-success">
                        <div class="panel-heading">{{pathSuffix}}</div>
                        <div class="panel-body">
                            <div class="row ">

                                {{>queryParams}}
                                {{>pathParams}}
                                {{>responseExample}}

                            </div>
                        </div>
                    </div>
                </div>
            {{/getMethods}}
            {{#postMethods}}
                <div class="row">
                    <div class="panel panel-primary">
                        <div class="panel-heading">{{pathSuffix}}</div>
                        <div class="panel-body">
                            <div class="row ">

                                {{>queryParams}}
                                {{>pathParams}}
                                {{>responseExample}}

                            </div>

                        </div>
                    </div>
                </div>
            {{/postMethods}}
        </div>
        {{#putMethods}}
            <div class="row">
                <div class="panel panel-info">
                    <div class="panel-heading">{{pathSuffix}}</div>
                    <div class="panel-body">
                        <div class="row ">

                            {{>queryParams}}
                            {{>pathParams}}
                            {{>responseExample}}

                        </div>
                    </div>
                </div>
            </div>
        {{/putMethods}}
    {{/resources}}

</div>
</body>
</html>